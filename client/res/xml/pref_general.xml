<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <EditTextPreference
        android:capitalize="words"
        android:defaultValue="@string/pref_default_timewindow"
        android:inputType="number"
        android:key="edit_timewindow"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/pref_title_display_name"
        android:summary="Select how many days backwards" />

    <com.jjdcorp.juvenalduarte.investmentadvisor.ListPreferenceMultiSelect
        android:defaultValue="ALL"
        android:key="mlist_feeders"
        android:title="Feeders"
        android:dialogTitle="Feeders"
        android:summary="Select jornals to retrieve articles"
        android:entries="@array/pref_example_list_values"
        android:entryValues="@array/pref_example_list_values" />

</PreferenceScreen>
